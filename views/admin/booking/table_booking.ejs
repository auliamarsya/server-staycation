<!-- DataTales -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Booking Table</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Invoice</th>
                        <th>Date</th>
                        <th>Title</th>
                        <th>Member Name</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% Object.keys(booking).forEach(key => { %>
                        <tr>
                            <td><%= parseInt(key) + 1 %></td>
                            <td><%= booking[key].invoice %></td>
                            <td><%= booking[key].bookingStartDate.toLocaleString().split(',')[0] %> s/d <%= booking[key].bookingEndDate.toLocaleString().split(',')[0] %></td>
                            <td><%= booking[key].itemId.title %></td>
                            <td><%= booking[key].memberId.firstName %> <%= booking[key].memberId.lastName %></td>
                            <td><%= booking[key].payments.status %></td>
                            <td>
                                <a href="/admin/booking/<%= booking[key].id %>" class="btn btn-success btn-circle btn-sm"><i class="fas fa-eye"></i></a>
                            </td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<!-- End DataTables -->