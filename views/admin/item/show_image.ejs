<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="show-image-tab">
    <div class="card shadow mb-4 mt-2">
        <div class="card-body">
            <div class="row">
                <% Object.keys(item.imageId).forEach(key=> { %>
                    <div class="col-sm-4">
                        <div class="card shadow mb-4 mt-2">
                            <div class="card-body">
                                <img src="https://iam-admin-staycation.herokuapp.com/<%= item.imageId[key].imageUrl %>" alt="image<%= key %>"
                                    style="width: 100%; display:block;">
                            </div>
                        </div>
                    </div>
                <% }); %>
            </div>
        </div>
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>